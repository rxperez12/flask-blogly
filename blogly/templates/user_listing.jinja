{% extends 'base.jinja' %}

{% block content %}

<h1>Users</h1>

<ul>
{% for user in users %}
      <li><a href="/users/{{ user.id }}">{{ user.get_full_name()}}</a></li>
{% endfor %}
</ul>

<button class="btn btn-primary" onclick="window.location.href='/users/new';">Add new user</button>


{% endblock %}